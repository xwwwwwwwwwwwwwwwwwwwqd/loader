   Section = AddSection(Tab2o, {"Player"})






local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Bi·∫øn c·ª•c b·ªô ƒë·ªÉ tr√°nh xung ƒë·ªôt
local selectedPlayer = nil
local playerList = {}
local playerDropdown -- Bi·∫øn l∆∞u dropdown

-- H√†m c·∫≠p nh·∫≠t danh s√°ch ng∆∞·ªùi ch∆°i
local function updatePlayerList()
    playerList = {} -- X√≥a danh s√°ch c≈©
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(playerList, player.Name)
        end
    end
    if playerDropdown then
        playerDropdown:SetOptions(playerList) -- C·∫≠p nh·∫≠t dropdown v·ªõi danh s√°ch m·ªõi
    end
end

updatePlayerList()
Players.PlayerAdded:Connect(updatePlayerList)
Players.PlayerRemoving:Connect(updatePlayerList)

-- N√∫t c·∫≠p nh·∫≠t danh s√°ch ng∆∞·ªùi ch∆°i trong Tab7o
AddButton(Tab2o, {
    Name = "Update Player",
    Callback = function()
        updatePlayerList()
    end
})

-- Dropdown danh s√°ch ng∆∞·ªùi ch∆°i trong Tab2o
playerDropdown = AddDropdown(Tab2o, {
    Name = "All Player",
    Options = playerList,
    Callback = function(selected)
        selectedPlayer = Players:FindFirstChild(selected)
    end
})

-- N√∫t d·ªãch chuy·ªÉn ƒë·∫øn ng∆∞·ªùi ch∆°i ƒë√£ ch·ªçn trong Tab2o
AddButton(Tab2o, {
    Name = "Teleport",
    Callback = function()
        if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
            LocalPlayer.Character.HumanoidRootPart.CFrame = selectedPlayer.Character.HumanoidRootPart.CFrame
        else
            warn("Kh√¥ng th·ªÉ d·ªãch chuy·ªÉn! Ng∆∞·ªùi ch∆°i kh√¥ng h·ª£p l·ªá ho·∫∑c ƒë√£ r·ªùi game.")
        end
    end
})
